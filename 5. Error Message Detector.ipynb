   "source": [
    "def error_message_detector(logs):\n",
    "    error_count = 0\n",
    "\n",
    "    for log in logs:\n",
    "        if log == \"ERROR\":\n",
    "            error_count += 1\n",
    "\n",
    "    print(\"Total number of ERROR entries:\", error_count)\n",
    "\n",
    "\n",
    "logs = [\"INFO\", \"ERROR\", \"WARNING\", \"ERROR\"]\n",
    "error_message_detector(logs)\n"
   ]
